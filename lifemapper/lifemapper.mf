   assemblePAM.py myPAM.npy Species_0.pav Species_1.pav Species_2.pav Species_3.pav Species_4.pav Species_5.pav Species_6.pav Species_7.pav Species_8.pav Species_9.pav Species_10.pav Species_11.pav Species_12.pav Species_13.pav Species_14.pav Species_15.pav Species_16.pav Species_17.pav Species_18.pav Species_19.pav
