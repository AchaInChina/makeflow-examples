#Get points for Species_0
Species_0.csv : 
   retrievePoints.py Species_0 Species_0.csv
#Create model for Species_0
Species_0.txt : Species_0.csv
   modelSpecies.py Species_0.csv --scenario=20 --algorithm=MAXENT -o Species_0.txt
#Create projection for Species_0
Species_0.tif : Species_0.txt
   projectSpecies.py Species_0.txt --scenario=21 -o Species_0.tif
#Get values for intersection
Species_0.vals : Species_0.csv Species_0.tif
   getIntersectValues.py Species_0.tif Species_0.csv -o Species_0.vals
#Intersect Species_0 with the shapegrid
Species_0.pav : Shapegrid.shp Species_0.tif Species_0.vals
   intersect.py Shapegrid.shp Species_0.tif -o Species_0.pav -v Species_0.vals

