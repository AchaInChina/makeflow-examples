   randomizePAM.py myPAM.npy -o randPAM1.npy
#Calculate stats for random PAM 1
randPAM1.stats : randPAM1.npy
   calculate.py randPAM1.npy -o randPAM1.stats
#Randomize PAM 2
randPAM2.npy : myPAM.npy
   randomizePAM.py myPAM.npy -o randPAM2.npy
#Calculate stats for random PAM 2
